{
  "name": "datagovmy-front",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md}": "prettier --write"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "eslint-config-custom": "*",
    "husky": ">=6",
    "lint-staged": ">=10",
    "postcss": "^8.4.21",
    "prettier": "latest",
    "tailwindcss": "^3.3.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
