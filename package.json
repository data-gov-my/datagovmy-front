{
  "name": "datagovmy-front",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "tests/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "e2e:dev": "turbo run e2e:dev",
    "e2e:prod": "turbo run e2e:prod",
    "e2e:vercel": "turbo run e2e:vercel",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,md}": "prettier --write"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "eslint-config-datagovmy": "*",
    "husky": ">=6",
    "inquirer-recursive": "^0.0.7",
    "lint-staged": ">=10",
    "plop": "^3.1.2",
    "postcss": "^8.4.21",
    "prettier": "latest",
    "tailwindcss": "^3.3.1",
    "turbo": "^1.10.2",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
